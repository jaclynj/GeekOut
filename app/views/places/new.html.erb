<div class = "form">
<h2> Add New Outing </h2>
<%= form_for @place do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :date %>
<%= f.text_field :date %>
<%= f.label :start_time %>
<%= f.text_field :start_time %>
<%= f.label :sponsor %>
<%= f.text_field :sponsor %>
<%= f.label :location %>
<%= f.select(:location, options_for_select([['New York, NY', 'New York, NY'], ['Philadelphia, PA','Philadelphia, PA']] )) %>
<%= f.label 'Venue Address' %>
<%= f.text_field :venue_address %>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.label :link %>
<%= f.text_field :link %>
<%= f.label :tags, "Tags (Seperated by commas)" %>
<%= f.text_field :tags, value: "" %>
<%= f.submit %>
<% end %>
</div>
